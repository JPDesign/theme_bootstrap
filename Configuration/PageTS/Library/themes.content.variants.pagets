themes {
	content {
		# Variants for content elements
		variants {
			default {
				framed = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.framed
				framed-small = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.framed_small
				framed-large = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.framed_large
			}
			# Special variant for Header
			# Used by ctype header
			# Will be merged with default
			header {
				page-header = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.page_header
				alert-success = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_success
				alert-info = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_info
				alert-warning = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_warning
				alert-danger = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_danger
			}
			# ctype: text
			text {
				wide-screen = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.wide_screen
				alert-success = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_success
				alert-info = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_info
				alert-warning = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_warning
				alert-danger = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_danger
			}
			# ctype: textpic
			textpic {
				wide-screen = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.wide_screen
			}
			# ctype: image
			image {
				wide-screen = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.wide_screen
			}
			# ctype: gridelements_pi1
			gridelements_pi1 {
				tabs {
					navigation-pills-color1 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_color1
					navigation-pills-color2 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_color2
					navigation-pills-color3 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_color3
					navigation-pills-color4 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_color4
					navigation-pills-color5 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_color5
					navigation-pills-color6 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_color6
				}
				container {
					wide-screen = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.wide_screen
					alert-success = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_success
					alert-info = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_info
					alert-warning = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_warning
					alert-danger = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.variants.alert_danger
				}
				singleColumnHeaderFooter {
					panel-color1 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_color1
					panel-color2 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_color2
					panel-color3 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_color3
					panel-color4 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_color4
					panel-color5 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_color5
					panel-color6 = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_color6
				}
			}
		}
	}
}